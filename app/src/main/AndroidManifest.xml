<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gmat.terminator">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".app.LevelApplication"
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
    <activity
        android:name=".activity.IntroSliderActivity"
        android:label="@string/app_name"
        android:theme="@style/AppTheme.NoActionBar">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
        <activity android:name=".activity.RegistrationActivity" />

        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.SelectTopicActivity" />
        <activity android:name=".activity.TimerActivity" />
        <activity android:name=".activity.QuestionCountActivity" />
    </application>
</manifest>